{
  "name": "device-sizes",
  "description": "Collection of properties of popular mobile devices.",
  "version": "0.0.0-development",
  "repository": "github:tobua/device-sizes",
  "license": "MIT",
  "author": "Matthias Giger",
  "scripts": {
    "build": "tsc index.ts --outDir dist --declaration --target esnext --module esnext",
    "start": "vercel dev"
  },
  "devDependencies": {
    "typescript": "^5.4.5",
    "vercel": "^34.2.7"
  },
  "type": "module",
  "sideEffects": false,
  "main": "dist/index.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "keywords": [
    "mobile",
    "device",
    "size",
    "resolution",
    "scale",
    "responsive"
  ],
  "prettier": {
    "printWidth": 100,
    "semi": false,
    "singleQuote": true
  },
  "publishConfig": {
    "provenance": true
  }
}
